@Article{MutzPemantle2015,
  author    = {Diana C. Mutz and Robin Pemantle},
  title     = {Standards for Experimental Research: Encouraging a Better Understanding of Experimental Methods},
  journal   = {Journal of Experimental Political Science},
  year      = {2015},
  volume    = {2},
  number    = {2},
  pages     = {192--215},
  abstract  = {In this essay, we closely examine three aspects of the Reporting Guidelines for this journal, as
described by Gerber et al. (2014, Journal of Experimental Political Science 1(1): 81–98) in the
inaugural issue of the Journal of Experimental Political Science. These include manipulation
checks and when the reporting of response rates is appropriate. The third, most critical,
issue concerns the committee’s recommendations for detecting errors in randomization. This
is an area where there is evidence of widespread confusion about experimental methods
throughout our major journals. Given that a goal of the Journal of Experimental Political
Science is promoting best practices and a better understanding of experimental methods
across the discipline, we recommend changes to the Standards that will allow the journal to
play a leading role in correcting these misunderstandings.},
  doi       = {10.1017/xps.2015.4},
  file      = {:Mutz, Pemantle (JEPS, 2015).pdf:PDF;:Mutz, Pemantle (Working Paper, 2011).pdf:PDF},
  publisher = {Cambridge University Press},
  timestamp = {2017-08-08},
  url       = {https://doi.org/10.1017%2Fxps.2015.4},
}

@Article{WeissgerberMilicWinhamGarovic2015,
  author    = {Tracey L. Weissgerber and Natasa M. Milic and Stacey J. Winham and Vesna D. Garovic},
  title     = {Beyond Bar and Line Graphs: Time for a New Data Presentation Paradigm},
  journal   = {{PLOS} Biology},
  year      = {2015},
  volume    = {13},
  number    = {4},
  pages     = {e1002128},
  month     = {apr},
  abstract  = {Figures in scientific publications are critically important because they often show the data
supporting key findings. Our systematic review of research articles published in top physiology
journals (n = 703) suggests that, as scientists, we urgently need to change our practices
for presenting continuous data in small sample size studies. Papers rarely included scatterplots,
box plots, and histograms that allow readers to critically evaluate continuous data.
Most papers presented continuous data in bar and line graphs. This is problematic, as
many different data distributions can lead to the same bar or line graph. The full data may
suggest different conclusions from the summary statistics. We recommend training investigators
in data presentation, encouraging a more complete presentation of data, and changing
journal editorial policies. Investigators can quickly make univariate scatterplots for small
sample size studies using our Excel templates.},
  doi       = {10.1371/journal.pbio.1002128},
  file      = {:Weissgerber, Milic, Winham, Garovic (PLoS Biol, 2015).pdf:PDF},
  publisher = {Public Library of Science},
  timestamp = {2017-08-08},
  url       = {https://doi.org/10.1371%2Fjournal.pbio.1002128},
}

