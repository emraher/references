@Unpublished{Boudreau2015,
  Title                    = {Party versus Principle: How Competing Parties and Frames Affect the Consistency between Citizens' Values and Policy Views},
  Author                   = {Boudreau, Cheryl},
  Note                     = {University of California, Irvine},
  Year                     = {2015},

  Abstract                 = {In representative democracies, political parties, candidates, and other elites seek to influence public opinion by publicizing their policy positions and framing them in ways that appeal to citizens' values. What effects do these competing policy positions and frames have on the consistency between citizens' opinions and their values? I address this question by conducting survey experiments where citizens express opinions about policies that involve a conflict between two values. I manipulate whether they receive the parties' competing positions, competing frames highlighting the values at stake, both, or neither type of information. I find that citizens do not simply follow their party when its position is inconsistent with their values. Rather, they follow a frame that provides a reason for supporting the other position, as long as that frame is not sponsored by the opposing party. Together, these results identify conditions under which citizens use signals from political parties and frames to express opinions that are consistent with their values.},
  File                     = {:Boudreau (Working paper, 2015).pdf:PDF},
  Timestamp                = {2015.01.26}
}

