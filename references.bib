@Article{Galdi2012,
  Title                    = {Selective Exposure in Decided and Undecided Individuals: Differential Relations to Automatic Associations and Conscious Beliefs},
  Author                   = {Galdi, Silvia and Gawronski, Bertram and Arcuri, Luciano and Friese, Malte},
  Journal                  = {Personality \& Social Psychology Bulletin},
  Year                     = {2012},

  Month                    = may,
  Number                   = {5},
  Pages                    = {559--69},
  Volume                   = {38},

  Abstract                 = {People often show a preference for information that confirms their attitudes and beliefs, and this tendency is reduced for opinions that are not held with conviction. The present study shows that both decided and undecided individuals show a tendency to selectively expose themselves to confirmatory information, albeit with different antecedents and consequences. Whereas selective exposure in decided participants was predicted by conscious beliefs and not by automatic associations, selective exposure in undecided participants was predicted by automatic associations and not by conscious beliefs. Moreover, selective exposure led undecided participants to adopt conscious beliefs that were in line with their preexisting automatic associations. Conversely, for decided participants, selective exposure shifted automatic associations in a direction that was in line with their preexisting conscious beliefs. Implications for decision making and mutual influences of automatic associations and conscious beliefs in attitude change are discussed.},
  Doi                      = {10.1177/0146167211435981},
  File                     = {:Galdi, Gawronski, Arcuri, Friese (PSPB, 2012).pdf:pdf},
  ISSN                     = {1552-7433},
  Keywords                 = {2010,2011,a variety of,attitude change,confirmation bias,decision making,decisions is central to,implicit measures,predicting future choices and,received october 31,revision accepted november 21,selective exposure},
  Pmid                     = {22389431},
  Url                      = {http://www.ncbi.nlm.nih.gov/pubmed/22389431}
}

