@Article{Durante2008,
  Title                    = {Changes in Women's Choice of Dress Across the Ovulatory Cycle: Naturalistic and Laboratory Task-Based Evidence},
  Author                   = {Durante, Kristina M. and Li, Norman P. and Haselton, Martie G.},
