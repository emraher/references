@Unpublished{GogginHendersonTheodoridis5200,
  Title                    = {Party Guessed? Assessing Party Ownership of Issues and Traits with a Conjoint Classification Experiment},
  Author                   = {Goggin, Stephen N. and Henderson, John A. and Theodoridis, Alexander G.},
  Note                     = {University of California--Berkeley},
  Year                     = {2015},

  Abstract                 = {A key component of the literature on issue and trait ownership of political parties is the cognitive association of political party with other information. In fact, many of the implications of ownership for candidate and party strategy, as well as the behavior of voters rely upon the presence of such associations with issue priorities and biographical information. Despite this, we know little about their existence or relative magnitude. We use a novel conjoint classification experiment that manipulates issue priorities and biographical details of hypothetical candidates and asks respondents to guess the party of the candidate. With this design, we assess the relative associations of a variety of personal and policy information with the Democratic and Republican parties, as well as with liberal and conservative labels. In addition to these associations, we assess the relative impact of this variety of personal and policy information on candidate evaluations. We find important new evidence that the implications of the ownership literature rely upon associations that clearly exist in some domains, yet are absent in others, and often moderated by voters' own partisanship.},
  File                     = {:Goggin, Henderson, Theodoridis (Working paper, 2015).pdf:PDF}
}

