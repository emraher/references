@InProceedings{FongImaiRatkovic2014,
  Title                    = {Covariate Balancing Propensity Scores for Multi-Valued Treatment Specifications},
  Author                   = {Fong, Christian and Imai, Kosuke and Ratkovic, Marc},
  Booktitle                = {Annual Meeting of the Midwest Political Science Association},
  Year                     = {2014},

  Address                  = {Chicago, IL},

  Abstract                 = {Many treatment regimes are non-binary. When researchers believe these non-binary treatments are not randomly assigned and wish to use propensity score techniques, they often dichotomize the treatment or use multinomial logistic regression to estimate multinomial propensity scores. However, dichotomizing the treatment may obscure important variation between the treatment groups, and the estimates from propensity score weighting with multinomial logistic regression are sensitive to slight misspecifications of the propensity score model. I derive a procedure for estimating propensity scores for multi-valued treatments under the covariate balancing propensity score (CBPS) framework proposed by Imai and Rakovic (2014). My simulations show that CBPS for multi-valued treatments outperforms maximum likelihood estimation. In an application to the recent debate in the Journal of Politics over the effects of education on political participation, I use CBPS for multi-valued treatments to disentangle the effect of attending college from the effect of finishing college. An implementation of the method developed here is available on CRAN.},
  File                     = {:Fong, Imai, Ratkovic (MPSA, 2014).pdf:PDF},
  Timestamp                = {2014.03.27}
}

