@Article{BerinskyMargolisSances2014,
  Title                    = {Separating the Shirkers from the Workers? Making Sure Respondents Pay Attention on Internet Surveys},
  Author                   = {Berinsky, Adam J. and Margolis, Michele F. and Sances, Michael W.},
  Journal                  = {American Journal of Political Science},
  Year                     = {2014},

