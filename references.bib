@Article{Uncapher2005,
  Title                    = {Encoding and the Durability of Episodic Memory: A Functional Magnetic Resonance Imaging Study},
  Author                   = {Uncapher, Melina R and Rugg, Michael D},
  Journal                  = {Journal of Neuroscience},
  Year                     = {2005},

  Month                    = aug,
  Number                   = {31},
  Pages                    = {7260--7},
  Volume                   = {25},

  Abstract                 = {Memories vary in their durability even when encoding conditions apparently remain constant. We investigated whether, under these circumstances, memory durability is nonetheless associated with variation in the neural activity elicited during encoding. Event-related functional magnetic resonance imaging data were acquired while volunteers semantically classified visually presented words. Using the "remember/know" procedure, memory for one-half of the words was tested after 30 min and for the remaining half after 48 h. In several regions, including left hippocampus and left dorsal inferior frontal gyrus (IFG), activity at encoding differed depending on whether items were later recollected regardless of study-test delay. Delay-selective effects were also evident, however. Recollection after 48 h was associated with enhanced activity in bilateral ventral IFG, whereas recollection after 30 min was associated with greater fusiform activity. Thus, there is a relationship between the neural activity elicited by an event as it is encoded and the durability of the resulting memory representation.},
  Doi                      = {10.1523/JNEUROSCI.1641-05.2005},
  File                     = {:Uncapher, Rugg (J Neuroscience, 2005).pdf:pdf},
  ISSN                     = {1529-2401},
  Keywords                 = {Adult, Brain, Brain: physiology, Female, Humans, Language, Magnetic Resonance Imaging, Male, Memory, Mental Recall, Mental Recall: physiology, Pattern Recognition, Short-Term, Short-Term: physiology, Time Factors, Visual, Visual: physiology, psychology},
  Owner                    = {Thomas},
