@Unpublished{LauRedlawskDitonto2014,
  Title                    = {Advantages and Disadvantages of Multi-Investigator Multi-Site (MIMS) Experiments},
  Author                   = {Lau, Richard R. and Redlawsk, David P. and Ditonto, Tessa M.},
  Note                     = {Unpublished manuscript, Rutgers University},

  Abstract                 = {A Multi-Investigator Multi-Site (MIMS) experiment gathers together a number of collaborators with ideas that can be tested simultaneously in the same study. A single experiment is developed with random manipulations designed to test all of those ideas, and then each collaborator runs an agreed-up number of subjects through the study at their home institution. The data from the different research sites are subsequently combined into a single dataset which is available to all of the multiple collaborators. This paper discusses the advantages and disadvantages of MIMS experiments in general, describes the design of one particular MIMS project, and then demonstrates several examples of one of the most important potential advantages of MIMS projects - greater statistical power due to more systematic variation in the data.},
  File                     = {:Lau, Redlawsk, Ditonto (Working paper, 2014).pdf:PDF},
  Timestamp                = {2014.10.24}
}

