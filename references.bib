@Article{Smith2007,
  Title                    = {The Role of Information-Processing Capacity and Goals in Attitude-Congruent Selective Exposure Effects},
  Author                   = {Smith, Steven M. and Fabrigar, Leandre R. and Potter, W. James and Estrada, Marie-J\"{o}elle},
  Journal                  = {Personality and Social Psychology Bulletin},
  Year                     = {2007},

  Month                    = jul,
  Number                   = {7},
  Pages                    = {948--60},
  Volume                   = {33},

  Abstract                 = {Two experiments explored the role of information-processing capacity and strategies in regulating attitude-congruent selective exposure. In Experiment 1, participants were placed under time pressure and randomly assigned to conditions in which either an attitude-expressive or no-information processing goal was made salient. Analyses revealed an attitude-congruent selective exposure effect and indicated that this effect was stronger when an attitude-expressive goal was made salient than when no goal was made salient. In Experiment 2, information-processing goals and time pressure were factorially manipulated. Analyses revealed an attitude-congruent selective exposure effect and indicated that this effect was especially strong when time pressure was high and an attitude-expressive goal was made salient. In both experiments, bias at exposure was found to predict bias at later stages of information processing (attention and memory). Supplementary analyses and data confirmed that the attitude-expressive goal manipulation activated its intended motivational processing strategy.},
  Doi                      = {10.1177/0146167207301012},
  File                     = {:Smith, Fabrigar, Powell, Estrada (Pers Soc Psy Bulletin, 2007).pdf:pdf},
  ISSN                     = {0146-1672},
  Keywords                 = {Attention, Attitude, Canada, Cognition, Goals, Humans, prospectus, psychology, selective exposure, Dissertation},
  Owner                    = {Thomas},
  Pmid                     = {17548525},
  Timestamp                = {2013.12.11},
  Url                      = {http://www.ncbi.nlm.nih.gov/pubmed/17548525}
}

