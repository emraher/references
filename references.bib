@Article{BerryDeMerittEsarey2010,
  Title                    = {Testing for Interaction in Binary Logit and Probit Models: Is a Product Term Necessary?},
  Author                   = {Berry, William D. and DeMeritt, Jacqueline H.R. and Esarey, Justin},
  Journal                  = {American Journal of Political Science},
  Year                     = {2010},

  Month                    = jan,
  Number                   = {1},
  Pages                    = {1},
  Volume                   = {54},

  Abstract                 = {Political scientists presenting binary dependent variable (BDV) models often hypothesize that variables interact to influence the probability of an event, Pr(Y). The current typical approach to testing such hypotheses is (1) estimate a logit or probit model with a product term, (2) test the hypothesis by determining whether the coefficient for this term is statistically significant, and (3) characterize the nature of any interaction detected by describing how the estimated effect of one variable on Pr(Y) varies with the value of another. This approach makes a statistically significant product term necessary to support the interaction hypothesis. We show that a statistically significant product term is neither necessary nor sufficient for variables to interact meaningfully in influencing Pr(Y). Indeed, even when a logit or probit model contains no product term, the effect of one variable on Pr(Y) may be strongly related to the value of another. We present a strategy for testing for interaction in a BDV model, including guidance on when to include a product term.},
  File                     = {:Berry, DeMeritt, Esarey (AJPS, 2010).pdf:PDF},
  Timestamp                = {2014.03.15}
}

@Article{BerryDeMerittEsarey2014,
  Title                    = {Bias and Overconfidence in Parametric Models of Interactive Processes},
  Author                   = {Berry, William D. and DeMeritt, Jacqueline H.R. and Esarey, Justin},
  Journal                  = {American Journal of Political Science},
  Year                     = {2014},

  Abstract                 = {When the structure of a data generating process (DGP) is empirically known or dictated by a strong theory, recent methodological scholarship provides guidance when testing for interaction between independent variables in their evariable. But statistical models are often used to `let the data speak for themselves' and must adapt to and recover relationships from a largely unknown DGP; in this situation, existing guidance does not apply. We find that parametric model dependence is a strong influence on measures of interaction in these scenarios. Some models are strongly overconfident, and all provide misleading indicators of the strength of interaction. We lay out a procedure for making the most effective use of parametric models when testing for interaction, show an example of this procedure in applied research, and argue for the development of less model-dependent estimators suited for this situation.},
  File                     = {:Berry, Demeritt, Esarey (AJPS, 2014).pdf:PDF},
  Timestamp                = {2014.03.15}
}

