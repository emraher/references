@Unpublished{PeerSamatBrandimarteAcquisti2015,
  author   = {Peer, Eyal and Samat, Sonam and Brandimarte, Laura and Acquisti, Alessandro},
  title    = {Beyond the Turk: An Empirical Comparison of Alternative Platforms for Crowdsourcing Online},
  year     = {2015},
  note     = {Unpublished paper, Bar-Ilan University},
  abstract = {Scholars in and outside behavioral sciences have been using online crowdsourcing services such as Amazon Mechanical Turk (MTurk) for online research. By and large, MTurk has been found to produce high quality data, but its success has come at a cost: some of MTurk's major emerging drawbacks include its slow replenishing rate and the growing non-naivety of participants to typical research materials. Recently, other services have emerged that offer similar capabilities to MTurk while promising access to new and more na{\"i}ve populations. We empirically compared several alternative crowdsourcing services, aiming to provide a preliminary investigation on the potential of these services to replace or supplement MTurk for online psychological research. Whereas some sites proved unusable, we found two (CrowdFlower and MicroWorkers) that showed adequate to high data quality, albeit sometimes inferior to MTurk's. Between the sites, we also found important psychometric and demographic differences that should be taken into account when planning to conduct behavioral research online.},
  file     = {:Peer, Samat, Brandimarte, Acquisti (SSRN, 2015).pdf:PDF}
}

