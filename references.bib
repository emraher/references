@Unpublished{FreederLenzTurney2014,
  Title                    = {The Importance of Knowing `What Goes With What': Reinterpreting the Evidence on Policy Voting and Multi-Item Issue Scales},
  Author                   = {Freeder, Sean and Lenz, Gabriel S. and Turney, Shad},
  Note                     = {UC-Berkeley},
  Year                     = {2014},

  Abstract                 = {Do citizens have meaningful views about public policy? Do those views influence their voting decisions? While Converse (1964) and others found voters wanting in both capacities, recent work byAnsolabehere, Snyder and Rodden (2008) suggests the opposite: after correcting for measurement errorwith multi-item scales, issue preferences appear stable and predictive of vote choice, even for voterswith low education and low political knowledge. Using a wide array of survey data, we show that neither claim holds. A large segment of the public, we find, lacks stable policy views that predict their vote choices, even after correcting for measurement error. They are so lacking because they are ignorant of which issue positions go with which candidate and party---of ``what goes with what.'' Despite Converse's focus on it, researchers have largely missed the critical role this information plays in attitude stability and constraint, and its normative implications for democracy.},
  File                     = {:Freeder, Lenz, Turney (Working paper, 2014).pdf:PDF},
  Timestamp                = {2014.09.07}
}

