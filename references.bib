@Article{BovitzDruckmanLupia2002,
  Title                    = {When Can a News Organization Lead Public Opinion? Ideology Versus Market Forces in Decisions to Make News},
  Author                   = {Bovitz, Gregory L. and Druckman, James N. and Lupia, Arthur},
  Journal                  = {Public Choice},
  Year                     = {2002},
  Pages                    = {127--155},
  Volume                   = {113},

  Abstract                 = {Do news organizations purposefully lead the public to support a particular ideo- logical agenda? When debating this question, many analysts draw conclusions from weak empirical evidence. We introduce a model that clarifies how a news organization's internal structure combines with market forces to affect when it can lead public opinion. We identify conditions under which liberal reporters or politically-driven media magnates can achieve ideological goals. We also illuminate important barriers that prevent many would-be public opinion leaders from ever satisfying these conditions. We show that internal structure and market forces are critical determinants of any news organization's power over public opinion.},
  File                     = {:Bovitz, Druckman, Lupia (Public Choice, 2002).pdf:pdf}
}

