  Volume                   = {21},

  Abstract                 = {Numerous studies have demonstrated that the attitudes people consider personally important have an enhanced impact on cognition and behavior. This article explores teh possibility hat importance may regulate the magnitude of the false consesnus effect as well. Teh authros reprot on six experiments tha tinvovled a variety of subject samples (college studenst and the general population) , data collection methods (telephone interviewing and self-administered questionnaires), and political issues (e.g., abortion, gun control, defense spending). Meta-analyses summarizing the results of 12 tests revealed a strong false consensus effect but no reliable relation between its magnitude and attitude importance. Tehse results are inconsistent with the assumption taht the false consensus effect is a result of attitudes directly or indirectly influencing perceptions of others and they lend support ot explanations of the false consensus effect that posit other mechanisms (i.e., attirbution adn object construal).},
  File                     = {:Fabrigar, Krosnick (PSPB, 1995).pdf:PDF},
  Timestamp                = {2015.11.20}
}

@Article{Fagerholm2015,
