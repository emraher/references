@Article{Palmer1999,
  author    = {Palmer, Barbara},
  title     = {Issue Fluidity and Agenda Setting on the Warren Court},
  journal   = {PRQ},
  year      = {1999},
  volume    = {52},
  number    = {1},
  pages     = {39--65},
  abstract  = {While justices have considerable discretion in the picking and choos- ing between cases in the creation of their agenda, how much discretion do they exercise in the picking and choosing between the issues that were presented by the parties, or even in developing new issues? Given the Warren Court's reputation for articulating broad rules that went beyond the particular circumstances of an individual case, especially in the area of civil rights and liberties, we would expect that this issue fluidity, particulary the expansion of issues, occurred with some frequency dur- ing this era. With an analysis of a random sample of 200 cases from the Warren Court, I found that the justices developed new issues in about one-fourth of all cases, while they suppressed issues that had been fully briefed and argued in over half of all cases. In addition, surprisingly, the Warren Court tended to suppress issues in civil rights and civil liberties cases, rather than expand them.},
  file      = {:Palmer (PRQ, 1999).pdf:PDF},
  timestamp = {2017-08-09},
  url       = {http://www.jstor.org/stable/449171},
}

@Article{Paluck2010,
  author    = {Elizabeth Levy Paluck},
  title     = {The Promising Integration of Qualitative Methods and Field Experiments},
  journal   = {The {ANNALS} of the American Academy of Political and Social Science},
  year      = {2010},
  volume    = {628},
  number    = {1},
  pages     = {59--71},
  month     = {feb},
  abstract  = {Randomized field experiments should take a more cen- tral place in qualitative research. Although field experi- mentation is often considered a quantitative enterprise, this paper illustrates the compatibility of field experi- mentation with various types of qualitative measurement tools and research questions. Integrating qualitative quantitative data within field experiments allows investi- gators to move past simple average treatment effects explore mechanisms of the identified causal effect. more novel proposal is to use field experimentation the organizing methodological framework for archival, ethnographic, or interpretive work, and to use ethno- graphic methods as the primary source of measurement in "experimental ethnography." Sustained research theoretical specificity can address some of the seemingly incompatible features of qualitative and field experimen- tal methods. For example, small sample sizes are accept- able as part of a research program, and some theories historical patterns or rare events could be disaggregated into smaller cause-and-effect linkages to test with field experiments in theoretically relevant contexts.},
